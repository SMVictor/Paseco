<div id="nested-fields" class="col-12 fields nested-fields text-center tbl_row" data-employee="<%= @employee.to_json(:include => { :positions => { :include => { :area => { :only => :name } }, :only => :name }, :stalls => {:only => :name } }) %>">
  <div class="row">
    <div class="col-xl-7 col-md-12">
      <div class="row">
        <div class="col-xl-4 col-md-6 mt-2">
          <label for="">Inicio</label>
          <%= f.text_field :start_date, id: "date", placeholder: "26/01/2020", autocomplete: "off", class: "form-control text-center", "data-date-format": 'dd/mm/yyyy', "data-provide": 'datepicker', "data-date-language": 'es', required: true, oninvalid: "setCustomValidity('Seleccione una fecha de inicio.')", onchange: "setCustomValidity('')", title: "Seleccione una fecha de inicio." %>
        </div>
        <div class="col-xl-4 col-md-6 mt-2">
          <label for="">Finalización</label>
          <%= f.text_field :end_date, id: "end_date", placeholder: "26/01/2020", autocomplete: "off", class: "form-control text-center", "data-date-format": 'dd/mm/yyyy', "data-provide": 'datepicker', "data-date-language": 'es', required: true, oninvalid: "setCustomValidity('Seleccione una fecha de fin.')", onchange: "setCustomValidity('')", title: "Seleccione una fecha de fin." %>
        </div>
        <div class="col-xl-2 col-md-6 mt-2">
          <label for="">Solicitados</label>
          <%= f.text_field :requested_days, id: "requested_days", class: "form-control text-center", placeholder: "11", required: true, oninvalid: "setCustomValidity('Ingrese la cantidad de días solicitados.')", onchange: "setCustomValidity('')", title: "Ingrese la cantidad de días solicitados." %>
        </div>
        <div class="col-xl-2 col-md-6 mt-2">
          <label for="">Libres</label>
          <%= f.text_field :included_freedays, id: "included_freedays", class: "form-control text-center", placeholder: "2", required: true, oninvalid: "setCustomValidity('Ingrese la cantidad de días libres incluidos en el periodo.')", onchange: "setCustomValidity('')", title: "Ingrese la cantidad de días libres incluidos en el periodo." %>
        </div>
      </div>
    </div>
    <div class="col-xl-2 col-md-6  mt-2">
       <label for="">Fecha</label>
      <%= f.text_field :date, id: "date", placeholder: "26/01/2020", autocomplete: "off", class: "form-control text-center", "data-date-format": 'dd/mm/yyyy', "data-provide": 'datepicker', "data-date-language": 'es', required: true, oninvalid: "setCustomValidity('Seleccione una fecha.')", onchange: "setCustomValidity('')", title: "Seleccione una fecha." %>
    </div> 
    <div class="col-xl-2 col-md-6  mt-2">
      <label for="">Periodo</label><br>
      <%= f.text_field :period, id: "period", class: "form-control text-center", placeholder: "2018-2019", title: "Ingrese el periodo.", required: true, oninvalid: "setCustomValidity('Ingrese el periodo.')", onchange: "setCustomValidity('')" %>
    </div>      
    <div class="col-xl-1 col-md-12  mt-2">
      <label for="">Acciones</label><br>
      <% if f.object.id && @employee.entries.last %>
        <button id="test" type="button" class="fa fa-eye" style="color: #8068D9;" onclick="generatePDFFile(this, '<%= @employee.entries.last.start_date %>', '<%= @employee.entries.last.end_date ? @employee.entries.last.end_date : active %>')"></button>
      <% end %>
      <%= link_to_remove_association "", f, class: "fas fa-trash-alt", id: 'remove_link', style: "color: red" %>
    </div>
  </div>
</div>