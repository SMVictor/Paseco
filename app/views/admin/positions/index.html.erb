<div class="row justify-content-center">
  <div class="col-10 mb-5">
    <%if notice%>
      <p class="alert alert-primary" role="alert" id="notice"><%= notice %></p>
    <% end %>
    <h2 class="d-sm-inline mr-5">Rangos</h2>
      <% if @positions.length > 10%>
        <%= link_to('Nuevo Rango', new_admin_position_path, class: 'btn primary-btn d-sm-inline')%>
      <% end -%>
  </div>
</div>
<div class="row justify-content-center" >
  <div class="col-12 col-md-9" style="">
    <div class="row justify-content-center">
      <div class="col-12 m-2">
        <div id="positionstable"><%= render 'admin/positions/partials/table', locals: {positions:  @positions}%></div>
      </div>
    </div>
  </div>
</div>