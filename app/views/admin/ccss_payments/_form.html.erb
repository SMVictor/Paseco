<div class="card">
  <div class="card-body">
    <div class="row form_fieldset">
      <div class="col-lg-4 mb-2 mt-2">
        <label for="">Porcentaje Normal</label>
        <%= form.text_field :percentage, id: "percentage", class: "form-control", placeholder: "10.34" %>
        <label id="percentage_error"></label>
      </div>
      <div class="col-lg-4 mb-2 mt-2">
        <label for="">Porcentaje Pencionados</label>
        <%= form.text_field :retired_percentage, id: "percentage", class: "form-control", placeholder: "10.34" %>
        <label id="percentage_error"></label>
      </div>
      <div class="col-lg-4 mb-2 mt-2">
        <label for="">Monto</label>
        <%= form.text_field :amount, id: "amount", class: "form-control", placeholder: "12750" %>
        <label id="amount_error"></label>
      </div>
    </div>
    <%= form.submit "Guardar", id: 'btn_submit', class: "btn btn-success mt-4" %>
    <%= link_to('Cancelar', root_path, class: 'btn btn-light ml-sm-3 mt-4')%>
  </div>
</div>
<script type="text/javascript">
  onInput("date");
  onInput("plan");
  onInput("shipping");
  onInput("concept");
</script>
