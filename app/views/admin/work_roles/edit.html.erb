<div class="bg-body-light">
    <div class="content content-full">
        <div class="d-flex flex-column flex-sm-row justify-content-sm-between align-items-sm-center">
            <h3 class="flex-sm-fill font-size-h2 font-w400 mt-2 mb-0 mb-sm-2">Editar Roll</h3>
            <nav class="flex-sm-00-auto ml-sm-3" aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <%= link_to('Roles', admin_work_roles_path, class: 'breadcrumb-item')%>
                    <li class="breadcrumb-item active" aria-current="page">Editar Roll</li>
                </ol>
            </nav>
        </div>
    </div>
</div>
<div class="d-flex justify-content-center">
  <div class="col-sm-12">
    <%= form_for(@role, url: admin_work_role_path(@role), html: {multipart: true, onsubmit:"return validateRoleForm()"}) do |form| %>
      <%= render 'admin/work_roles/partials/form', form: form %>
    <% end %>
  </div>
</div>