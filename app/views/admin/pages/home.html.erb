<% if flash[:notice] %>
  <div class="alert alert-success alert-dismissible fade show mt-4 d-flex justify-content-center" role="alert">
    <%= flash[:notice] %>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
<% end %>
<% if @unregistered_employees.length > 0 && current_user.admin? %>

  <%= form_tag do %>
  <div id="home_table"><%= render 'admin/pages/partials/home_table', locals: {unregistered_employees:  @unregistered_employees}%></div>
  <% end %>
<% end %>