<div  class="alert alert-info alert-dismissible fade show mt-4" role="alert">
  <h4 class="alert-heading">Alerta! Los siguientes empleados tienen la cuenta sin registrar: </h4>
  <div class="row d-none d-md-flex mt-4 text-md-center tb_head" style="border: 1px solid #E8EBF2;">
    <div class="th col-2">
      <p class="text-uppercase font-weight-bold pt-3">Empleado</p>
    </div>
    <div class="th col-2">
      <p class="text-uppercase font-weight-bold pt-3">Puesto</p>
    </div>
    <div class="th col-2">
      <p class="text-uppercase font-weight-bold pt-3">Titular</p>
    </div>
    <div class="th col-2">
      <p class="text-uppercase font-weight-bold pt-3">CÃ©dula</p>
    </div>
    <div class="th col-3">
      <p class="text-uppercase font-weight-bold pt-3">Cuenta</p>
    </div>
    <div class="th col-1">
      <p class="text-uppercase font-weight-bold pt-3">Registrar</p>
    </div>
  </div>
  <div class="row tb_body">
    <% @unregistered_employees.each_with_index do |employee, index| %>
      <div class="td col-2 pt-md-3">
        <p><%= employee.name %></p>
      </div>
      <div class="td col-2 pt-md-3">
        <p><%= employee.stalls[0].name %></p>
      </div>
      <div class="td col-2 pt-md-3">
        <p><%= employee.account_owner != ""? employee.account_owner : employee.name %></p>
      </div>
      <div class="td col-2 pt-md-3">
        <p><%= employee.account_identification != ""? employee.account_identification : employee.identification %></p>
      </div>
      <div class="td col-3 pt-md-3">
        <p><%= employee.formated_account %></p>
      </div>
      <div class="td col-1 pt-md-3">
        <input type="checkbox" name="register" onchange="registerEmployee(<%= employee.id %>)">
      </div>
    <% end %>
  </div>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>