<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
    <Row>
      <Cell><Data ss:Type="String"><%= BacInfo.complete_firts("B" + @bac_info.plan + @bac_info.shipping) + "00000" + @bac_info.date + BacInfo.complete_total(@total.to_s) + BacInfo.complete_total_records(@count.to_s) %></Data></Cell>
    </Row>
    <% @position = 0 %>
    <% @payrole.payrole_lines.each_with_index do |payrole, index| %>

      <% @employee = Employee.find(payrole.employee_id ) %>

      <% if @employee.bank == "BAC" && payrole.net_salary.to_i > 0 %>
      <% @position += 1 %>
        <Row>

          <% if payrole.employee.account_owner != nil && payrole.employee.account_owner != "" %>
            <% @account_owner = payrole.employee.account_owner %>
          <% else %>
            <% @account_owner = payrole.employee.name %>
          <% end %>

          <% if @employee.account_identification != nil && @employee.account_identification != "" %>
            <% @account_identification = @employee.account_identification %>
          <% else %>
            <% @account_identification = @employee.identification %>
          <% end %>
          <Cell><Data ss:Type="String"><%= BacInfo.complete_firts("T" + @bac_info.plan + @bac_info.shipping + BacInfo.complete_identification(@account_identification, @employee.id_type)) + BacInfo.complete_index((@position).to_s) + @bac_info.date +  BacInfo.complete_total((payrole.net_salary.to_i * 100).to_s)  +  "     " + BacInfo.complete_concept(@bac_info.concept + " " + BacInfo.fix_name(payrole.employee.name)) + BacInfo.fix_name(@account_owner) %></Data></Cell>
        </Row>
      <% end %>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>