<div class="row justify-content-center" >
  <div class="col-12" style="">
    <% if flash[:notice] %>
          <div class="alert alert-success alert-dismissible fade show mt-4 d-flex justify-content-center" role="alert">
            <%= flash[:notice] %>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
      <% end %>
    <div class="row justify-content-center">
      <div class="col-lg-12 col-md-9 col-sm-9 col-xs-9">
        <legend class="text-left">Distribuci√≥n de Oficiales en <%= @stall.name %></legend>
        <%= select_tag "role[role_line_employee]", options_from_collection_for_select(@stall.employees.where(active: true).order(name: :asc), "id", "name"), html_options = { class: "form-control mt-4", id: "employee_select", prompt: "Seleccione un Empleado", onchange: "loadEmployee("+@role.id.to_s+", "+@stall.id.to_s+")" } %>
        <div id="role_lines" data-requirements="<%= @stall.quote.requirements.to_json(include: :shift) %>" data-date_lines="<%= @role.role_lines.where(stall: @stall).to_json  %>" data-positions="<%= Position.all.to_json %>"><%= render 'admin/roles/partials/role_lines_form' %></div>
      </div>
    </div>
  </div>
</div>