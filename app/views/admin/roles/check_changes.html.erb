<div class="card">
  <div class="card-body">
    <div class="d-flex justify-content-center text-center mt-5">
      <h3 class="d-none d-md-flex">Revisión de modificaciones para: <%= @stall.name %></h3>
      <h5 class="d-md-none">Revisión de modificaciones para: <%= @stall.name %></h5>
    </div>
    <% if flash[:notice] %>
	    <div class="alert alert-success alert-dismissible fade show mt-4 d-flex justify-content-center" role="alert">
	      <%= flash[:notice] %>
	        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
	          <span aria-hidden="true">&times;</span>
	        </button>
	    </div>
	  <% end %>
    <div id="rolesTable" class="mt-5"><%= render 'admin/roles/partials/changes_table', locals: {changes:  @changes}%></div>
  </div>
</div>